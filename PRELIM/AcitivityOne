import java.util.*;

public class TableAndChairs {
    public static void main(String[] args) {
    
    
   System.out.println("x               x");
   System.out.println("x               x");
   System.out.println("xxxxx       xxxxx");
   System.out.println("x   x       x   x");
   System.out.println("x   x       x   x");

 }
}
